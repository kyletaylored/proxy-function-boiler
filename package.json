{
  "name": "proxy-boiler",
  "version": "1.0.0",
  "description": "Proxy server for cloud services.",
  "main": "src/index.js",
  "scripts": {
    "start": "node_modules/.bin/nodemon --inspect --no-colors node_modules/.bin/functions-framework --target=app --port 8001",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "gcloud functions deploy <function-name> --entry-point app --runtime nodejs10 --trigger-http --allow-unauthenticated --vpc-connector <vpc-connector-id> --egress-settings all"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json"
  },
  "author": "John Doe <john.doe@example.com>",
  "license": "MIT",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:prettier/recommended",
      "eslint:recommende"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-underscore-dangle": [
        "error",
        {
          "allowAfterThis": true
        }
      ],
      "prettier/prettier": "error"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^1.5.1",
    "@google-cloud/logging-winston": "^3.0.6",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chalk": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5"
  }
}